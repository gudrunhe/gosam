process_name=dgzd
process_path=dgzd

# PDG codes # Gluon: 21 # Higgs-Boson: 25
in=1, 21
out=23, 1

order=QCD, 1, 3

model=smdiag
model.options=  GF:0.00001166390,mW:80.419,mZ:91.188,wW:0,wZ:0

one=gs,e
zero=mU,mD,mS,mC,mB,me,mmu

qgraf.options=onshell,notadpole,nosnail

# No Higgs propagators
qgraf.verbatim=true=iprop[H,0,0];

symmetries=family

regularisation_scheme=cdr
r2=implicit

extensions=gaugecheck
