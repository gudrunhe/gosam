process_name=dgzd
process_path=dgzd

# PDG codes # Gluon: 21 # Higgs-Boson: 25
in=1, 21
out=23, 1

order=QCD, 1, 3

model=smdiag
model.options=  GF:0.00001166390,mW:80.419,mZ:91.188,wW:0,wZ:0

one=gs,e
zero=mU,mD,mS,mC,mB,me,mmu

# No Higgs propagators
filter.particles=H:0

symmetries=family

regularisation_scheme=dred
convert_to_thv=true
r2=implicit

extensions=gaugecheck
