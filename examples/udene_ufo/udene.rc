process_name=udene
process_path=virtual

# PDG codes
# u: 2, d: 1
# Gluon: 21
# Z-Boson: 23
# e-: 11, \nu_e: 12
in=-2, 1
out=11, -12

order=QCD, 0, 2

# Only W+ in s-channel, no Goldstone bosons
filter.lo=lambda d: d.bridge(["W+", "W-"]) > 0
filter.nlo=lambda d: d.bridge(["W+", "W-"]) > 0


model=FeynRules, ../model/Standard_Model_UFO

model.options=

# There's only one non-zero helicity
helicities=+--+
