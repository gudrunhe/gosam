@end @if %]

[% @if generate_lo_diagrams %]
SOURCE_TREE= \
	diagramsl0[% @if use_order_names %]_0.f90 diagramsl0_1.f90 diagramsl0_2[% @end @if %].f90 $(DIAGRAMS_TREE)[%
