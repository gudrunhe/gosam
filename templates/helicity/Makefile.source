        d[%$_%]h[%helicity%]l132_qp.f90 \
        d[%$_%]h[%helicity%]l121_qp.f90[%
      @end @if %][%
   @end @if %][%
   @end @if %][%
   @end @for %][%
@end @select %][%
@end @if %]

TXT_VIRT=[%
@select r2
@case implicit explicit off %]$(subst .f90,.txt,$(DIAGRAMS_VIRT))[%
@case only %][%
   @for elements topolopy.keep.virt %] \
	d[%$_%][% @if use_order_names %]_0h[%helicity%]l1.abb \
	d[%$_%]_1h[%helicity%]l1.abb \
	d[%$_%]_2[% @end @if %]h[%helicity%]l1.abb[%
   @end @for %][%
@end @select %]

ABB_VIRT=[%
@if helsum %][%
@else %][%
   @select abbrev.level
   @case helicity %]abbrevh[%helicity%][% @if use_order_names %]_0.f90 abbrevh[%helicity%]_1.f90 abbrevh[%helicity%]_2[% @end @if %].f90[%
   @case group %][%
      @for groups var=grp %] \
	abbrevg[%grp%]h[%helicity%][% @if use_order_names %]_0.f90 abbrevg[%grp%]h[%helicity%]_1.f90 abbrevg[%grp%]h[%helicity%]_2[% @end @if %].f90[%
      @end @for %][%
   @case diagram %][%
      @for elements topolopy.keep.virt %] \
	abbrevd[%$_%][% @if use_order_names %]_0h[%helicity%].f90 abbrevd[%$_%]_1h[%helicity%].f90 abbrevd[%$_%]_2[% @end @if %]h[%helicity%].f90[%
      @end @for %][%
   @end @select %][%
  @if extension quadruple %][%
   @select abbrev.level
   @case helicity %]abbrevh[%helicity%][% @if use_order_names %]_0_qp.f90 abbrevh[%helicity%]_1_qp.f90 abbrevh[%helicity%]_2[% @end @if %]_qp.f90[%
   @case group %][%
      @for groups var=grp %] \
	abbrevg[%grp%]h[%helicity%][% @if use_order_names %]_0_qp.f90 abbrevg[%grp%]h[%helicity%]_2_qp.f90 abbrevg[%grp%]h[%helicity%]_2[% @end @if %]_qp.f90[%
      @end @for %][%
   @case diagram %][%
      @for elements topolopy.keep.virt %] \
	abbrevd[%$_%][% @if use_order_names %]_0h[%helicity%]_qp.f90 abbrevd[%$_%]_1h[%helicity%]_qp.f90 abbrevd[%$_%]_2[% @end @if %]h[%helicity%]_qp.f90[%
      @end @for %][%
   @end @select %][%
 @end @if extension quadruple %][%
@end @if helsum %]
SOURCE_VIRT=$(ABB_VIRT) $(DIAGRAMS_VIRT)
[% @else %]
DIAGRAMS_VIRT=
TXT_VIRT=
SOURCE_VIRT=
[% @end @if generate_nlo_virt %]
[% @if generate_uv_counterterms %]
DIAGRAMS_CT=[%
   @for elements topolopy.keep.ct %] \
	d[%$_%][% @if use_order_names %]_0h[%helicity%]l1c.f90 d[%$_%]_2h[%helicity%]l1c.f90 d[%$_%]_2[% @end @if %]h[%helicity%]l1c.f90[%
   @end @for %]
TXT_CT=$(subst .f90,.txt,$(DIAGRAMS_CT))
SOURCE_CT=$(ABB_CT) $(DIAGRAMS_CT)[%
